<% for(var i = 0; i < users.length; i++) { %>
<tr>
    <td><%=i + 1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].register %></td>
    <td>
        <a href="/asis_user?id=<%= users[i]._id%>" class="btn border-shadow update">
            <span class="text4"><i class="fa-solid fa-qrcode"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%= users[i]._id%>>
            <span class="text4"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>